/* pages/myCode/myCode.wxss */

.input-wrapper {
  margin: 40rpx auto 0 auto;
  width: 600rpx;
  height: 88rpx;
  border: 2rpx solid #ddd;
  border-radius: 16rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
}

.input-wrapper input {
  font-size: 32rpx;
  line-height: 88rpx;
  height: 100%;
}

.button-wrapper {
  margin-top: 40rpx;
}
.canvas {
  margin: 20rpx auto;
  width: 150px;
  height: 150px;
  /* border: 1px solid black; */
}

.tip {
  text-align: center;
}
.MyVIPCard{
  height: 400rpx;
  background:white;
  border-radius: 15rpx 15rpx;
  margin: 20rpx 20rpx;
  box-shadow: 5px 5px 5px #cdcfcf;
}
.cardhead{
  border-radius: 15rpx 15rpx;
  height: 40%;
  background: -webkit-linear-gradient(left,black,rgb(27, 34, 37),white);
}